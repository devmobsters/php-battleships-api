    "description": "API for the Battleships game, implemented in PHP.",
    "license": "MIT",
    "type": "project",
    "autoload": {
        "psr-4": { "App\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\": "tests/" }
    },
    "require": {
        "symfony/symfony": "3.3.*",
